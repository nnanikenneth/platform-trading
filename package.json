{
  "devDependencies": {
    "@nestjs/testing": "^10.4.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/passport-jwt": "^4.0.1",
    "dotenv-cli": "^7.4.2",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.{js,ts,json,md}\"",
    "migrate": "ts-node scripts/migrate.ts",
    "seed": "ts-node scripts/seed.ts",
    "start": " npx ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development dotenv -e .env ts-node -r tsconfig-paths/register src/main.ts",
    "start:prod": "npm run build && NODE_ENV=production dotenv -e .env.production node dist/main.js",
    "migrate:dev": "dotenv -e .env npx prisma migrate dev",
    "migrate:prod": "dotenv -e .env.production npx prisma migrate deploy",
    "test": "jest"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "@fastify/compress": "^8.0.1",
    "@fastify/helmet": "^12.0.1",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.4",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.20.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "fastify-cors": "^6.0.3",
    "fastify-rate-limit": "^5.8.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "npm": "^10.9.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.20.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
